{
    "source": {
        "include": [
            "README.md",
            "src"
        ],
        "includePattern": ".+jsx?"
    },
    "opts": {
        "destination": "./docs/",
        "recurse": true,
        "template": "node_modules/jsdonk",
        "jsdonk": {
            "title": "react-entity-editor",
            "sections": {
                "module": {
                    "typedef": false
                },
                "typedef": false
            }
        }
    },
    "plugins": [
        "plugins/markdown",
        "node_modules/jsdoc-babel",
        "node_modules/jsdonk/lib/plugins/react-tags",
        "node_modules/jsdoc-react-proptypes/lib/plugin"
    ],
    "babel": {
        "extensions": [
            "js",
            "es6",
            "jsx"
        ],
        "presets": [
            "es2015",
            "react"
        ],
        "plugins": [
            "transform-flow-strip-types"
        ]
    }
}
